ar': 'img/default_avatar/avatar.jpg'},\r\n        {'name': '大奇', 'sex': '女', 'avatar': 'img/default_avatar/avatar.jpg'},\r\n        {'name': '小奇', 'sex': '男', 'avatar': 'img/default_avatar/avatar.jpg'},\r\n        {'name': '小奇', 'sex': '女', 'avatar': 'img/default_avatar/avatar.jpg'},\r\n        {'name': '大可', 'sex': '男', 'avatar': 'img/default_avatar/avatar.jpg'}]\r\n    export default {\r\n        name: \"hall\",\r\n\r\n        data() {\r\n            return {\r\n                Iconshow: false,   //chat按钮隐藏\r\n                users: json,   //在线用户\r\n                color: ['#ff709d', '#7fe6f5']  //性别颜色\r\n            }\r\n        },\r\n        mounted() {\r\n            let usersScroll = this.$refs.users  //允许滑动\r\n            new BScroll(usersScroll, {\r\n                click: true,\r\n            })\r\n        },\r\n        methods: {\r\n            display() {   //上拉下拉图标的隐藏显示\r\n                this.Iconshow = true\r\n                setTimeout(() => {\r\n                    this.Iconshow = false\r\n                }, 5000)\r\n            }\r\n        }\r\n    }\r\n</script>\r\n\r\n<style scoped lang=\"stylus\">\r\n    #hall\r\n        .main\r\n            .users\r\n                position relative\r\n                top 24vw\r\n                height 18vw\r\n                z-index 10\r\n                .user\r\n                    height 18vw\r\n                    width 100%\r\n                    border-bottom #c6c6c6 solid 1px\r\n\r\n                    &.woman\r\n                        background: #ff709d22\r\n\r\n                    &.man\r\n                        background: #3ec8ff 22\r\n\r\n                /* Safari 5.1 - 6.0 */\r\n\r\n                .avatar\r\n                    position absolute\r\n                    top 3vw\r\n                    left 6vw\r\n                    height 10vw\r\n                    width 10vw\r\n                    border-radius 50%\r\n\r\n                .name\r\n                    position absolute\r\n                    top 9vw\r\n                    left 20vw\r\n                    font-size 3vw\r\n                    color #d3dce6\r\n\r\n                .sex\r\n                    position absolute\r\n                    top 12vw\r\n                    left 15vw\r\n                    font-size 3vw\r\n                    font-weight bold\r\n                .btn\r\n                    position absolute\r\n                    top 1vw\r\n                    right 3vw\r\n                    .el-button\r\n                        margin 0\r\n                        margin-right 3px\r\n                        height 16vw\r\n                        width 18vw\r\n                        background #0000\r\n                        border: 0\r\n                        outline 0\r\n                        color #f0f0f0\r\n                        font-size 6vw\r\n\r\n\r\n            .chatBtn\r\n                position fixed\r\n                z-index 10\r\n                bottom 8em\r\n                right 1em\r\n                opacity 0.6\r\n                background #0000\r\n                .el-button\r\n                    font-size 4vw\r\n</style>\r\n"]}]}