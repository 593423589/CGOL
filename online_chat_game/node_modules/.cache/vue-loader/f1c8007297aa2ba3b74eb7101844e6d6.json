col>\r\n                </el-row>\r\n            </el-footer>\r\n        </el-container>\r\n    </div>\r\n</template>\r\n\r\n\r\n<script>\r\n    export default {\r\n        name: \"login\",\r\n        mounted() {\r\n        },\r\n        data() {\r\n            return {\r\n                log: ['登录', '注册', '没有账户？', '已注册?'],\r\n                register: true,\r\n                size: 50,\r\n                url: 'img/background/blur.png',\r\n                logUser: '',\r\n                logPassword: '',\r\n                logEnsurePassword: '',\r\n                color: '#5B696C'\r\n            }\r\n        },\r\n        methods: {\r\n            submit() {\r\n                let user = {user: this.logUser, password: this.logPassword}\r\n                this.axios.get('http://localhost:9999',user)\r\n                    .then(function (response) {\r\n                        console.log(response)\r\n                    })\r\n                    .catch(function (error) {\r\n                        console.log(error)\r\n\r\n                    })\r\n            },\r\n            exchange() {\r\n                this.register = !this.register\r\n            }\r\n        }\r\n    }\r\n</script>\r\n\r\n<style lang=\"stylus\">\r\n    #login\r\n        .background\r\n            position fixed\r\n            top 0\r\n            left 0\r\n            z-index -10\r\n\r\n        .header\r\n            width 100%\r\n            color #fcfcfc\r\n            font-size 15px\r\n            line-height 90px\r\n            margin-bottom 20px\r\n\r\n            .icon\r\n            .title\r\n                text-align center\r\n\r\n        .el-main\r\n            .avatar\r\n                margin-bottom 20px\r\n\r\n            .log\r\n                margin-top 10px\r\n                margin-bottom 10px\r\n\r\n                .el-autocomplete\r\n                    display block\r\n\r\n                .BtnCaptcha\r\n                    position relative\r\n                    top 6px\