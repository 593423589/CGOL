     if (this.register) {\r\n                    this.axios.post('http://localhost:9999/user', user)\r\n                        .then(function (response) {\r\n                            console.log(response)\r\n                        })\r\n                        .catch(function (error) {\r\n                            console.log(error)\r\n                        })\r\n                } else {\r\n                    if (this.logPassword.length >= 6) {\r\n                        if (this.logEnsurePassword === this.logPassword) {\r\n                            this.axios.post('http://localhost:9999/register', user)\r\n                                .then(response => {\r\n                                    if (response.data === 'success') {\r\n                                        this.$message({\r\n                                            message: '注册成功',\r\n                                            type: 'success'\r\n                                        });\r\n                                    } else if (response.data === 'fail') {\r\n                                        this.$message({\r\n                                            message: '用户名已经有人用啦',\r\n                                            type: 'warning'\r\n                                        });\r\n                                    }\r\n                                })\r\n                                .catch(function (error) {\r\n                                    throw error\r\n                                })\r\n                        } else {\r\n                            this.$message({\r\n                                showClose: true,\r\n                                message: '密码输入不一致',\r\n                                type: 'error'\r\n                            });\r\n                        }\r\n                    }else {\r\n                        this.$message({\r\n                            showClose: true,\r\n                            message: '密码最少6位',\r\n                            type: 'error'\r\n                        });\r\n\r\n                    }\r\n                }\r\n\r\n            },\r\n            exchange() {\r\n                this.register = !this.register\r\n            }\r\n        }\r\n    }\r\n</script>\r\n\r\n<style lang=\"stylus\">\r\n    #login\r\n        .background\r\n            position fixed\r\n            top 0\r\n            left 0\r\n            z-index -10\r\n\r\n        .header\r\n            width 100%\r\n            color #fcfcfc\r\n            font-size 15px\r\n            line-height 90px\r\n            margin-bottom 20px\r\n\r\n            .icon\r\n            .title\r\n                text-align center\r\n\r\n        .el-main\r\n            .avatar\r\n                margin-bottom 20px\r\n\r\n            .log\r\n                margin-top 10px\r\n                margin-bottom 10px\r\n\r\n                .el-autocomplete\r\n                    display block\r\n\r\n                .BtnCaptcha\r\n                    position relative\r\n                    top 6px\r\n                    right 4em\r\n                    background-color #fff0\r\n                    width 20px\r\n                    border 0\r\n                    color #222\r\n                    font-size 14px\r\n\r\n                .el-input__inner\r\n                    -webkit-appearance: none\r\n                    background-color: #fff0\r\n                    background-image: none\r\n                    border 0\r\n                    border-bottom 1px solid rgba(71, 97, 110, 0.72)\r\n                    border-radius 0\r\n                    -webkit-box-sizing: border-box\r\n                    display: inline-block\r\n                    padding 0\r\n                    padding-left 5px\r\n                    font-size: 14px\r\n                    height: 40px\r\n                    line-height: 40px\r\n                    outline: 0\r\n                    width: 70%\r\n                    -webk