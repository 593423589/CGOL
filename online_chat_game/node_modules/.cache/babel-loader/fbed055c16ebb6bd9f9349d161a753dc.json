oll: null,\n      commitBtnBadge: 0,\n      privateChat: {},\n      //私聊内容\n      privateChatBuffer: {},\n      //私聊内容缓存  解决卡慢的问题\n      privateChating: false,\n      //私聊框隐藏\n      privatePeople: {},\n      //私聊成员,\n      privateUnread: [] //未读消息\n\n    };\n  },\n  props: ['MessageContent', 'privateChatContent'],\n  mounted: function mounted() {\n    //允许滑动\n    this.usersScroll = new BScroll(this.$refs.users, {\n      click: true\n    });\n    this.messagesScroll = new BScroll(this.$refs.messages, {\n      click: true,\n      scrollbar: true,\n      scrollY: true,\n      stop: 0,\n      startY: -5000,\n      mouseWheel: {\n        speed: 20,\n        invert: false,\n        easeTime: 300\n      }\n    });\n    this.MessageInputDisplay = false;\n  },\n  methods: {\n    displayChat: function displayChat(event, name, avatar) {\n      var _this = this;\n\n      //群聊图标的隐藏显示\n      if (name && avatar) {\n        this.privateChating = true; //私聊框显示\n\n        this.privatePeople = {\n          name: name,\n          avatar: avatar\n        }; //给当前聊天框的对象赋值\n\n        this.privateUnread.splice(this.privateUnread.indexOf(this.name), 1); //删除未读红点提示\n      } else {\n        this.privateChating = false; //私聊框隐藏\n\n        this.commitBtnBadge = 0; //红点显示数字\n      }\n\n      this.Iconshow = true;\n      setTimeout(function () {\n        _this.Iconshow = false;\n      }, 5000);\n      this.MessageInputDisplay = true;\n    },\n    SendMessage: function SendMessage() {\n      var name = this.$parent.name;\n      var message = this.inputMessage;\n      var avatar = this.$parent.avatar;\n      var date = new Date().getHours() + ':' + new Date().getMinutes();\n\n      if (name) {\n        if (message) {\n          if (!this.privateChating) {\n            //发送群聊\n            this.$socket.emit('broadcast', {\n              'name': name,\n              'message': message,\n              'date': date,\n              'avatar': avatar\n            });\n          } else {\n            //发送私聊\n            var chatTo = this.privatePeople.name;\n            this.$socket.emit('privateChat', {\n              'chatFrom': name,\n              'message': message,\n              'date': date,\n              'chatTo': chatTo,\n              'name': name,\n              'avatar': avatar\n            });\n          }\n\n          this.inputMessage = '';\n        } else {\n          this.$message.info('写点东西再提交把！');\n        }\n      } else {\n        this.$message.info('请补充个人信息');\n      }\n    },\n    closeMessage: function closeMessage() {\n      this.MessageInputDisplay = false;\n    },\n    playGame: function playGame() {\n      this.$message.info({\n        showClose: true,\n        message: '即将开通，敬请期待'\n      });\n    }\n  },\n  watch: {\n    inputMessage: function inputMessage() {\n      this.inputBtnType = 'success';\n    },\n    MessageContent: function MessageContent() {\n      this.commitBtnBadge++;\n      var messagesList = this.$refs.messages.querySelectorAll('ul>li');\n      this.messagesScroll.scrollToElement(messagesList[messagesList.length - 1], 100, true, true);\n    },\n    privateChatContent: function privateChatContent() {\n      var chatFromData = this.privateChatContent;\n      var chat = chatFromData.chat;\n\n      if (this.privateChatBuffer[chat]) {\n        this.privateChatBuffer[chat].push(chatFromData);\n      } else {\n        this.privateChatBuffer[chat] = [chatFromData];\n      }\n\n      this.privateUnread.push(chatFromData.name);\n      this.privateChat = []; //先把聊天内容清空\n\n      this.privateChat = this.privateChatBuffer;\n    }\n  }\n};",{"version":3,"sources":["hall.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAkGA,OAAA,OAAA,MAAA,eAAA;AAEA,eAAA;AACA,EAAA,IAAA,EAAA,MADA;AAGA,EAAA,IAHA,kBAGA;AACA,WAAA;AACA,MAAA,QAAA,EAAA,KADA;AACA;AACA,MAAA,KAAA,EAAA,CAAA,SAAA,EAAA,SAAA,CAFA;AAEA;AACA,MAAA,YAAA,EAAA,EAHA;AAIA,MAAA,YAAA,EAAA,MAJA;AAKA,MAAA,mBAAA,EAAA,IA